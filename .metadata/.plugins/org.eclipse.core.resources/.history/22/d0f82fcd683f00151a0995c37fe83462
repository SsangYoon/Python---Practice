'''

def Checker(Value):
    if (Value) :
        print ("True")
    else :
        print ("False")
        
def String_Check():
    Checker ("")
    
def List_Check():
    print ([])
    print ([1, 2, 3])
    
def Tuple_Check():
    print (())
    print ((1, 2))

def Dictionary_Check():
    print ({})
    print ({1 : "One"})

def Sets_Check():
    print ({})
    print ({1, 2, "asdf"})

def Intergel_Check():
    if (0):
        print ("Fasle")
    else:
        print ("True")

def None_Check():
    if (None):
        print ("False")
    else:
        print ("True")


    
if __name__ == '__main__':
    String_Check ()
    List_Check ()
    Tuple_Check ()
    Dictionary_Check ()
    Sets_Check ()
    Intergel_Check ()
    None_Check ()
    
    
    
    
    
    