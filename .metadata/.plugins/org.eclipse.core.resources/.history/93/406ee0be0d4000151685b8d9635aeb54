def Multiplication_View(Number) : 
    Multiple_Number = 1
    
    while Multiple_Number < 10 :
        print ("%s X %s = %d" % (Number, Multiple_Number, Number * Multiple_Number))
        Multiple_Number++
if __name__ == '__main__':
    pass